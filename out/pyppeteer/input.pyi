from pyppeteer.connection import CDPSession as CDPSession
from pyppeteer.errors import PyppeteerError as PyppeteerError
from pyppeteer.us_keyboard_layout import keyDefinitions as keyDefinitions
from pyppeteer.util import merge_dict as merge_dict
from typing import Any, Dict

class Keyboard:
    def __init__(self, client: CDPSession) -> None: ...
    async def down(self, key: str, options: dict=..., **kwargs: Any) -> None: ...
    async def up(self, key: str) -> None: ...
    async def sendCharacter(self, char: str) -> None: ...
    async def type(self, text: str, options: Dict=..., **kwargs: Any) -> None: ...
    async def press(self, key: str, options: Dict=..., **kwargs: Any) -> None: ...

class Mouse:
    def __init__(self, client: CDPSession, keyboard: Keyboard) -> None: ...
    async def move(self, x: float, y: float, options: dict=..., **kwargs: Any) -> None: ...
    async def click(self, x: float, y: float, options: dict=..., **kwargs: Any) -> None: ...
    async def down(self, options: dict=..., **kwargs: Any) -> None: ...
    async def up(self, options: dict=..., **kwargs: Any) -> None: ...

class Touchscreen:
    def __init__(self, client: CDPSession, keyboard: Keyboard) -> None: ...
    async def tap(self, x: float, y: float) -> None: ...
